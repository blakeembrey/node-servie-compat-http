{
  "name": "servie-compat-http",
  "version": "0.2.0",
  "description": "Map Servie `Request` and `Response` instances to node.js HTTP compatible objects",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "lint": "tslint \"src/**/*.ts\"",
    "build": "rm -rf dist/ && tsc",
    "specs": "jest --coverage",
    "test": "npm run -s lint && npm run -s build && npm run -s specs",
    "prepublish": "typings install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/blakeembrey/node-servie-compat-http.git"
  },
  "keywords": [
    "request",
    "response",
    "http",
    "node",
    "compat",
    "req",
    "res",
    "server"
  ],
  "author": {
    "name": "Blake Embrey",
    "email": "hello@blakeembrey.com",
    "url": "http://blakeembrey.me"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/blakeembrey/node-servie-compat-http/issues"
  },
  "homepage": "https://github.com/blakeembrey/node-servie-compat-http",
  "devDependencies": {
    "express": "^4.15.0",
    "jest": "^19.0.0",
    "rimraf": "^2.5.4",
    "tslint": "^4.3.1",
    "tslint-config-standard": "^4.0.0",
    "typescript": "^2.1.5",
    "typings": "^2.1.0"
  },
  "dependencies": {
    "servie": "^0.2.0"
  }
}
